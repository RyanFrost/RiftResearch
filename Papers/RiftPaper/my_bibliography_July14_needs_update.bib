@Misc{Choi_2009_Neural,
  Title                    = {Neural control of locomotor adaptation in humans},

  Author                   = {Julia T. L. Choi},
  Note                     = {Copyright - Copyright ProQuest, UMI Dissertations Publishing 2009; Last updated - 2014-01-10; First page - n/a},
  Year                     = {2009},

  Abstract                 = {Locomotor patterns must be continuously adjusted to the immediate surrounds (e.g. uneven or slippery surface) and to changes in the body over time (e.g. growth and injury). In this thesis, I explored the neural mechanisms underlying adaptation of locomotion in humans. Adaptation was studied using a split-belt treadmill, where the two legs were constrained to separate belts moving at different or equal speeds. First, I investigated the extent to which walking in different directions can be adapted independently. It was surprising to discover that adaptation was direction-specific and did not transfer when the walking pattern 'simply' reversed. Moreover, adaptation was specific to a direction on each leg, rather than a specific set (i.e. forward-forward, forward-backward). Next, I found that adaptation transferred between different forms of forward locomotion (i.e. walk vs. run and level vs. incline walking). This finding is consistent with other studies suggesting that locomotor circuits are organized into "modules", some of which are shared across different forms of locomotion, and some are specialized. Finally, I studied split-belt walking in children with hemispherectomy (i.e. removal of one cerebral hemisphere). Adaptation of temporal, but not spatial, walking parameters were impaired in some hemispherectomy patients. This suggests that cerebral connections contribute to adaptation of timing in locomotion.},
  File                     = {Choi_2009_Neural.pdf:Papers by BibTEX ID\\In catalog (temp) Change JabRef folder when delete this\\Choi_2009_Neural.pdf:PDF},
  ISBN                     = {9781109128666},
  Journal                  = {ProQuest Dissertations and Theses},
  Language                 = {English},
  Pages                    = {127},
  Timestamp                = {2014.08.05}
}

