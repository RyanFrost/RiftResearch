@Article{Pohl_2007_Repetitive,
  Title                    = {Repetitive locomotor training and physiotherapy improve walking and basic activities of daily living after stroke: a single-blind, randomized multicentre trial (DEutsche GAngtrainerStudie, DEGAS)},
  Author                   = {Pohl, M and Werner, C and Holzgraefe, M and Kroczek, G and Wingendorf, I and Ho{\"o}lig, G and Koch, R and Hesse, S},
